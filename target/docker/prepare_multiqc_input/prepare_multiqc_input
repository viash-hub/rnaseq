$( if [ ! -z ${VIASH_PAR_PRESEQ_MULTIQC+x} ]; then echo "${VIASH_PAR_PRESEQ_MULTIQC}" | sed "s#'#'\"'\"'#g;s#.*#par_preseq_multiqc='&'#" ; else echo "# par_preseq_multiqc="; fi )
