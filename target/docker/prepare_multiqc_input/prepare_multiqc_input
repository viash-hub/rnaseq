$( if [ ! -z ${VIASH_PAR_MARKDUPLICATES_MULTIQC+x} ]; then echo "${VIASH_PAR_MARKDUPLICATES_MULTIQC}" | sed "s#'#'\"'\"'#g;s#.*#par_markduplicates_multiqc='&'#" ; else echo "# par_markduplicates_multiqc="; fi )
